{% extends 'admin/base.html.twig' %}

{% block title %}{{ city.name}}{% endblock %}

{% block body %}
<div class="container-fluid">
    <h1 class="my-5">Ville de {{ city.name}}</h1>
    <table class="table table-hover">
        <thead>
            <tr>
                <th>Nom</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>{{ city.name }}</td>
            </tr>
        </tbody>
    </table>
</div>
{% endblock %}
